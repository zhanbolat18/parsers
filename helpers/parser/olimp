<script>
            //переменная которая показывает, было ли сделано удаление коэфициента в карзину либо добавление
            var koefDeleted = false;
            //айди коэфициента, котрый был добавлен самым последним (чтобы отправлять данные только по нему)
            var last_koefID = "";


            $(document).ajaxSuccess(function (event, xhr, settings, data) {
                //перехват AJAX данных с карзины
                var url = (settings.url);
                var isbasket_responce = url.search("ajax_basket_index.php");
                if (isbasket_responce > (-1)) {
                    GA_basketAddKoef(data, last_koefID);
                    //определение совершенного действия в корзине
                    if (data.case == "addkef") {


                        if (koefDeleted == false) {
                            //добавлен коэфициент
                            GA_basketAddKoef(data, last_koefID);
                        } else {
                            //удален коэфициент
                            GA_basketRemovekoef(data, last_koefID);
                        }
                    }

                    if (data.case == "deleteevent") {
                        GA_basketRemovekoef(data);
                    }

                    if (data.case == "submit") {
                        GA_basketSubmit(data);
                    }

                    if (data.case == "form") {
                        GA_basketListKoefs(data);
                    }

                }

                //возвращаем значение переменной в исходное состояние
                koefDeleted = false;

            });


            //событие ДОБАВЛЕНИЕ КОЭФИЦИЕНТА
