                function set_lshow(start, last, pos, sport_id) {
                    var arrow_span = $('.sportArrowForSportId-' + sport_id);
                    if (arrow_span.hasClass('arrowDownDynamic')) {
                        //arrow_span.switchClass('arrowDownDynamic','arrowRightDynamic');
                        arrow_span.removeClass('arrowDownDynamic');
                        arrow_span.addClass('arrowRightDynamic');
                        $('.sportIdOfMatchTr-' + sport_id).show();
                        document.cookie = 'sport_arrow_hidden_' + sport_id + ' = 0;';
                    }
                    else {
                        //arrow_span.switchClass('arrowRightDynamic','arrowDownDynamic');
                        arrow_span.removeClass('arrowRightDynamic');
                        arrow_span.addClass('arrowDownDynamic');
                        $('.sportIdOfMatchTr-' + sport_id).hide();
                        document.cookie = 'sport_arrow_hidden_' + sport_id + ' = 1;';
                    }
                }
            </script> </form>
