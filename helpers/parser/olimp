<script language='JavaScript'>
                    var h_int_live = null;
                    function save_upd_live() {
                        var obj_refresh_live = document.getElementById('refresh_live');
                        if (obj_refresh_live) {
                            if (obj_refresh_live.checked) {
                                document.cookie = 'upd_disable_live=0;';
                                change_rr_live();
                            } else {
                                document.cookie = 'upd_disable_live=1;';
                                if (h_int_live) {
                                    clearInterval(h_int_live);
                                }
                            }
                        }
                    }
                    function ajax_reload_live() {
                        var ai = new AJAXInteraction('ajax_index.php?page=line&live=1', function (str) {
                            document.getElementById('central_td').innerHTML = str;
                            var arr = str.match(/v[0-9]{1,} = [0-9]{1,}(, vl = [0-9]{1,})?, vc[0-9]{1,} = [0-9]{1,}, ordc[0-9]{1,} = [0-9]{1,};/ig);
                            for (var i = 0; i < arr.length; i++) {
                                eval(arr[i]);
                            }
                        });
                        ai.doPost();
                    }
                    function change_rr_live() {
                        var rr = 30;
                        var obj_refresh_live = document.getElementById('refresh_live');
                        var obj_refrate_live = document.getElementById('refrate_live');
                        if (obj_refresh_live && obj_refrate_live) {
                            if (!obj_refresh_live.checked) {
                                if (h_int_live) {
                                    clearInterval(h_int_live);
                                }
                                return;
                            }
                            if (obj_refrate_live != 'undefined' && obj_refrate_live != null) {
                                rr = obj_refrate_live.value;
                            }
                            document.cookie = 'refrate_live=' + rr + ';';
                            if (h_int_live) {
                                clearInterval(h_int_live);
                            }
                            h_int_live = setInterval('ajax_reload_live()', rr * 1000);
                        }
                    }
                    document.addEventListener("DOMContentLoaded", function () {
                        change_rr_live();
                    });
                    function getCookie(name) {
                        var cookie = " " + document.cookie;
                        var search = " " + name + "=";
                        var setStr = null;
                        var offset = 0;
                        var end = 0;
                        if (cookie.length > 0) {
                            offset = cookie.indexOf(search);
                            if (offset != -1) {
                                offset += search.length;
                                end = cookie.indexOf(";", offset)
                                if (end == -1) {
                                    end = cookie.length;
                                }
                                setStr = unescape(cookie.substring(offset, end));
                            }
                        }
                        return (setStr);
                    }
                </script>
