                function set_lcheck(last, status) {
                    var intstatus = 0;
                    if (status) {
                        intstatus = 1;
                    }
                    else {
                        intstatus = 0;
                    }
                    var total_cb_count = document.getElementsByName("live[]").length;

                    $('input[name="live[]"]').each(function () {
                        if ($(this).is(":visible")) {
                            $(this).prop("checked", status);
                            cookMe($(this).attr('id'), intstatus);
                            console.log($(this).attr('id'));
                        }
                    });
                }
