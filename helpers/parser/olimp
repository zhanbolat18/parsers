<script>
                $(document).ready(function () {
                    //сворачивание виджета при нажатии на заголовок
                    $('.video-widget-header .title').on('click', function () {
                        var $content = $('.video-widget-body'),
                            speed = 500;
                        $content.stop(true, true);
                        if (false == $content.hasClass('is_active'))
                            $content.slideDown(speed, function () {
                                $(this).addClass('is_active');
                            });
                        else
                            $content.slideUp(speed / 2, function () {
                                $(this).removeClass('is_active');
                            });
                    });
                    //обрабатываем нажатие на кнопку отдельного видео окна
                    $('.video-widget-header .full-button').off();
                    $('.video-widget-header .full-button').on('click', function () {
                        window.open('index.php?page=video&id=' + glob_video_id, '', 'width=750,height=600,menubar=0,toolbar=0,directories=0,status=1,location=0,resizable=0,scrollbars=0');
                        $('#close_side_videobar').click();
                        $('.video-widget').hide();
                    });
                    //обрабатываем нажатие на кнопку стандартного/расширенного видео окна виджета
                    $('.video-widget-header .expand-button').off();
                    $('.video-widget-header .expand-button').on('click', function () {
                        ToggleTdWidth();
                        if ($('.col_td.right').hasClass('expanded')) {
                            $(this).attr('title', 'Minimize video stream');
                        }
                        else {
                            $(this).attr('title', 'Maximize video stream');
                        }
                        openvideobr(glob_video_id);
                    });
                    $('.video-widget-header .close-button').on('click', function () {
                        $('.video-widget-body').html('');
                        $('.video-widget').hide();
                    });
                    $('.video-widget-body').on('mouseover', function () {
                        $('.video-widget-title').text($('#match_live_name_' + glob_video_id).text());
                        $('.video-widget-title').fadeIn();
                    });
                    $('.video-widget-body').on('mouseout', function () {
                        $('.video-widget-title').fadeOut();
                    });
                    $('.video-widget-header .expand-button').on('mouseover', function () {
                        if ($('.col_td.right').hasClass('expanded')) {
                            $(this).attr('title', 'Minimize video stream');
                        }
                        else {
                            $(this).attr('title', 'Maximize video stream');
                        }
                });
                });
            </script>
