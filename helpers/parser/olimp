<script type="text/javascript">
            function getCookie(name) {
                var matches = document.cookie.match(new RegExp(
                    "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
                ));
                return matches ? decodeURIComponent(matches[1]) : undefined;
            }

            var vvv = getCookie("showlivefilter") == "none" ? 0 : 1;

            function sortLiveList() {
                if ($(".live_sports:checked").length == 0) {
                    cookMe("liveFilterEnabled", "0");
                } else {
                    cookMe("liveFilterEnabled", "1");
                }

                if (getCookie("liveFilterEnabled") == "1") {
                    $(".live_sports").each(function () {
                        if ($(this).is(":checked")) {
                            if ($(".sportArrowForSportId-" + $(this).data("sport")).hasClass('arrowRightDynamic')) {
                                $('.forLiveFilter[data-sport="' + $(this).data("sport") + '"]').show();
                            } else {
                                $('.forLiveFilter[data-sport="' + $(this).data("sport") + '"][data-kek="arrow"]').show();
                            }
                        } else {
                            $('.forLiveFilter[data-sport="' + $(this).data("sport") + '"]').hide();
                        }
                    });
                } else {
                    $(".forLiveFilter").each(function () {
                        if ($(this).data("kek") == "arrow") {
                            $(this).show();
                        } else {
                            if ($(".sportArrowForSportId-" + $(this).data("sport")).hasClass('arrowRightDynamic')) {
                                $(this).show();
                            }
                        }
                    });
                }
                if ($(".forLiveFilter").filter(":visible").length == 0) {
                    $("#noResults").show();
                    $("#submitLiveMatches").hide();
                    cookMe("hasResults", "none")
                } else {
                    $("#submitLiveMatches").show();
                    $("#noResults").hide();
                    cookMe("hasResults", "table-row")
                }
            }

            function check(field, flag) {
                if (flag == "1") {
                    $('input[type=checkbox][name="list"]').each(function () {
                        cookMe("livesport" + $(this).data("sport"), "checked");
                        $(this).prop("checked", true);
                    });
                    cookMe("checkIt", "checked");
                    $("#filterActivated").prop("checked", true);
                }
                else {
                    $('input[type=checkbox][name="list"]').each(function () {
                        cookMe("livesport" + $(this).data("sport"), "");
                        $(this).prop("checked", false);
                    });
                    cookMe("checkIt", "");
                    $("#filterActivated").prop("checked", false);
                }
                sortLiveList();
            }

            /*скрытие блока фильтра по видам спорта при клике за пределами*/


            function showSportsMenu() {
                console.log($("#showMe").css("display"));

                if ($("#showMe").hasClass("opened")) {
                    $("#showMe").css("display", "none");
                    $("#showMe").removeClass("opened");
                } else {
                    $("#showMe").css("display", "block");
                    $("#showMe").addClass("opened");
                }

                if ($("#showMe").is(":visible")) {
                    cookMe("showlivefilter", "block");
                } else {
                    cookMe("showlivefilter", "none");
                }
            }

            $(document).mouseup(function (e) {
                var container = $("#showMe");
                if (container.has(e.target).length === 0) {
                    container.hide();
                    vvv = 0;
                }
                if ($("#showMe").is(":visible")) {
                    cookMe("showlivefilter", "block");
                } else {
                    cookMe("showlivefilter", "none");
                }
            });

            function checkAll(aaa) {
                cookMe("checkIt", $('.live_sports[name="list"]:checked').length == 0 ? "" : "checked");
                $("#filterActivated").prop("checked", $('.live_sports[name="list"]:checked').length == 0 ? false : true);

                var el = $(aaa);
                if (el.is(":checked")) {
                    $(".sportArrowForSportId-" + el.data("sport")).show();
                    cookMe("livesport" + el.data("sport"), "checked");
                } else {
                    cookMe("livesport" + el.data("sport"), "");
                }
                sortLiveList();
            }

            function set_match_checked(matchid) {
                var obj_matchidcb = document.getElementById('matchidcb' + matchid);
                if (obj_matchidcb) {
                    if (obj_matchidcb.checked) {
                        document.cookie = "matchidcb" + matchid + "=1;";
                    }
                    else {
                        document.cookie = "matchidcb" + matchid + "=0;";
                    }
                }
            }

            function cookMe(name, val) {
                document.cookie = name + "=" + val;
            }
        </script>
